---
layout: none
---
<?xml version="1.0" encoding="utf-8" ?>
<feed xml:lang="en-US" xmlns="http://www.w3.org/2005/Atom">
    <id>{{ site.name | xml_escape }}</id>
    <title>{{ site.name | xml_escape }}</title>
    <author>
        <name>Jason Lee</name>
    </author>
    <updated>{{ site.date | date_to_rfc822 }}</updated>
    <link href="{{ site.url }}/feed.atom" rel="self" type="application/atom+xml" />
    <link href="{{ site.url }}" rel="alternate" type="text/html" />
    {% for post in site.posts limit:25 %}
        <entry>
            <id>{{ site.url }}{{ post.url }}</id>
            <title>{{ post.title | xml_escape }}</title>
            <updated>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</updated>
            <published>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</published>
            <link href="{{ site.url }}{{ post.url }}" rel="alternate" type="text/html" />
            <author>
                <name>Jason Lee</name>
            </author>
                {% for tag in post.tags %}
            <category term="{{ tag }}"/>
                {% endfor %}
            <summary>
                {{ post.excerpt | xml_escape }}
            </summary>
        </entry>
    {% endfor %}
</feed>